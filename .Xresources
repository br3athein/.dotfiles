XTerm*faceName: Terminus
XTerm*faceSize: 10
XTerm*vt100*geometry: 80x60
XTerm*saveLines: 16384
XTerm*loginShell: true
XTerm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
XTerm*termName: rxvt-unicode-256color
XTerm*eightBitInput: false

URxvt*depth: 32

URxvt.font: xft:Terminess Powerline:size=13, \
            xft:Terminus:size=13, \
            xft:WenQuanYi Bitmap Song:size=8, \
            xft:FreeSerif:style=Regular, \
            xft:Unifont:style=Medium:antialias=false

URxvt*vt100*geometry: 75x60

URxvt*saveLines: 16384
URxvt*loginShell: true
URxvt*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
URxvt*eightBitInput: false
URxvt*scrollBar: false

! Source: https://wiki.archlinux.org/index.php/rxvt-unicode
! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
! end source

! URxvt*scrollmode: plain
! URxvt*scrollBar_right: true
! URxvt*scrollColor: _colorIT

URxvt*termName: rxvt-unicode-256color
! URxvt*perl-ext:
URxvt*perl-ext-common: default,clipboard,selection-to-clipboard,matcher

URxvt.iso14755: False
URxvt.iso14755_52: False

URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste

URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

